generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  name     String
  email    String  @unique
  password String
  cpf      String  @unique
  isAdmin  Boolean @default(false)

  createAt DateTime @default(now())
  updateAt DateTime @updatedAt

 }